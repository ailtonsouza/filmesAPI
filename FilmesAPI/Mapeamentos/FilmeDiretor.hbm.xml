<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="FilmesAPI" namespace="FilmesAPI.Entidades">
    <class name="FilmeDiretor">
        <id name="Id">
            <generator class="identity"/>
        </id>

        <many-to-one name="Filme" column="FilmeId" fetch="join" not-null="true" />

        <many-to-one name="Diretor"  column="DiretorId" fetch="join" not-null="true"/>


    </class>
</hibernate-mapping>

