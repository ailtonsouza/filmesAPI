<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="FilmesAPI" namespace="FilmesAPI.Entidades">
    <class name="Oscar">
        <id name="Id">
            <generator class="identity"/>
        </id>
        <property name="Ano" not-null="true"/>
        <property name="Categoria" not-null="true"/>
        
        
    <many-to-one name="Filme" column="FilmeId" fetch="join" not-null="true"/>
  
    </class>

</hibernate-mapping>