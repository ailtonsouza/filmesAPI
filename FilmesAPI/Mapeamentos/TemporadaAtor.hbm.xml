<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="FilmesAPI" namespace="FilmesAPI.Entidades">
    <class name="TemporadaAtor">
        <id name="Id">
            <generator class="identity"/>
        </id>

        <property name="NomePersonagem" not-null="true"/>

        <many-to-one name="Temporada" column="TemporadaId" fetch="join" not-null="true" />

        <many-to-one name="Ator"  column="AtorId" fetch="join" not-null="true"/>


    </class>
</hibernate-mapping>