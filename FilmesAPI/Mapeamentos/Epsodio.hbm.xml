<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="FilmesAPI" namespace="FilmesAPI.Entidades">
    <class name="Epsodio">
        <id name="Id">
            <generator class="identity"/>
        </id>
        <property name="Titulo" not-null="true"/>
        <property name="Duracao" not-null="true"/>
        <property name="NumeroEpsodio" not-null="true"/>
        
        
      <many-to-one name="Temporada" column="TemporadaId" fetch="join" not-null="true"/>
 
        
        
        
        
        
    </class>
</hibernate-mapping>